use java::lang::System

class Pyramid {

}

impl Pyramid {
    comp {
        fn main(args: str[]) {
            rows := 5

            for mut i := 1; i <= rows; i = i + 1 {
                for mut j := 1; j <= i; j = j + 1 {
                    System.out.print(j)
                    System.out.print(" ")
                }
                System.out.println()
            }
        }
    }
}
